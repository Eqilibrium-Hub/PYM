<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_hotel_folio_form_inherit" model="ir.ui.view">
            <field name="name">hotel.folio.pym</field>
            <field name="model">hotel.folio</field>
            <field name="inherit_id" ref="hotel.view_hotel_folio_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='service_line_ids']/tree/field[@name='product_id']" position="after">
                    <field name="room_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>